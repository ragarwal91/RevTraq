<%= form_for @business do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.label :logo %>
  <%= f.text_field :logo %>

  <%= f.submit %>

<% end %>